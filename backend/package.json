{
  "name": "node-typescript-rest-api",
  "version": "1.0.0",
  "description": "Test Run 1.0",
  "scripts": {
    "start": "export PORT=80 || set PORT=80 && npm run transpile && node server.js",
    "dev": "concurrently \"npm:watch\" \"npm:serve\"",
    "mg": "npx prisma migrate dev",
    "serve": "nodemon server.js",
    "watch": "tsc -w",
    "transpile": "tsc",
    "seed": "npm run transpile && node src/db/prisma/seed.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "forever": "export PORT=80 || set PORT=80 && npm run transpile && forever start server.js"
  },
  "engines": {
    "node": ">=8.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/db/prisma/seed.ts",
    "schema": "src/db/prisma/schema.prisma"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nmanikiran/rest-api-node-typescript.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/helmet": "4.0.0",
    "@types/joi": "^17.2.2",
    "@types/node": "^15.0.1",
    "@types/winston": "^2.4.4",
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7",
    "prisma": "^4.2.1",
    "tslint": "^6.1.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@prisma/cli": "^2.16.0-dev.68",
    "@prisma/client": "^4.3.0-dev.15",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "africastalking": "^0.5.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^8.2.0",
    "express": "^4.18.1",
    "express-rate-limit": "^5.2.6",
    "forever": "^4.0.3",
    "helmet": "^4.5.0",
    "husky": "^6.0.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.6",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "socket.io": "^4.5.1",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^4.3.0",
    "toad-scheduler": "^1.6.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "hooks": {
    "pre-commit": "npm run lint"
  }
}
